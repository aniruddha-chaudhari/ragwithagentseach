{
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python",
            "config": {
                "excludeFiles": "**/*.ipynb **/*.log **/__pycache__/** **/test.py",
                "pythonVersion": "3.10"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
}